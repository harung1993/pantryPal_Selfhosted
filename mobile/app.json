{
  "expo": {
    "name": "PantryPal",
    "slug": "pantrypal",
    "version": "1.3.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "scheme": "pantrypal",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#fbbf24"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.palstack.pantrypal",
      "buildNumber": "4",
      "infoPlist": {
        "NSCameraUsageDescription": "PantryPal needs camera access to scan barcodes on your pantry items.",
        "NSPhotoLibraryUsageDescription": "PantryPal needs photo library access to save item images.",
        "ITSAppUsesNonExemptEncryption": false
      },
      "backgroundColor": "#fbbf24"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#fbbf24"
      },
      "package": "com.palstack.pantrypal",
      "permissions": [
        "CAMERA",
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE",
        "android.permission.CAMERA",
        "android.permission.RECORD_AUDIO"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "bundler": "metro"
    },
    "plugins": [
      [
        "expo-camera",
        {
          "cameraPermission": "Allow PantryPal to access your camera to scan barcodes."
        }
      ],
      [
        "expo-notifications",
        {
          "icon": "./assets/notification-icon.png",
          "color": "#ffffff",
          "sounds": []
        }
      ],
      "expo-secure-store"
    ],
    "extra": {
      "eas": {
        "projectId": "b2ace730-1f21-4da3-a5c2-97f8b867d7f8"
      }
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/b2ace730-1f21-4da3-a5c2-97f8b867d7f8"
    }
  }
}
